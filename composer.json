{
    "name": "horde/controller",
    "description": "Horde Controller libraries",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Mike Naberezny",
            "email": "mike@naberezny.com",
